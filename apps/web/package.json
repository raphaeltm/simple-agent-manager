{
  "name": "@simple-agent-manager/web",
  "version": "0.1.0",
  "private": true,
  "description": "Control plane UI for Simple Agent Manager",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "dev:mock": "vite",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint 'src/**/*.{ts,tsx}' 'tests/**/*.{ts,tsx}'",
    "deploy": "pnpm build && wrangler pages deploy dist",
    "deploy:staging": "pnpm build && wrangler pages deploy dist --env staging"
  },
  "dependencies": {
    "@simple-agent-manager/shared": "workspace:*",
    "@simple-agent-manager/terminal": "workspace:*",
    "@simple-agent-manager/ui": "workspace:*",
    "better-auth": "^1.0.0",
    "lucide-react": "^0.460.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-v8": "^2.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "jsdom": "^24.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^2.0.0",
    "wrangler": "^3.0.0"
  }
}
